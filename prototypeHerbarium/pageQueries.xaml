<Page x:Class="prototypeHerbarium.pageQueries"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:prototypeHerbarium"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="pageQueries">

    <DockPanel>
        <ComboBox DockPanel.Dock="Top" Margin="10" />
        <DataGrid Margin="10" Visibility="Visible">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Query Results" Width="*"/>
            </DataGrid.Columns>
        </DataGrid>
    </DockPanel>
</Page>
